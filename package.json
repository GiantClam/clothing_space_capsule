{
  "name": "clothing-space-capsule",
  "version": "1.0.0",
  "description": "虚拟试衣 Electron 应用",
  "main": "main.js",
  "scripts": {
    "electron": "electron .",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run api-server\" \"node start-electron.js\"",
    "server": "node server/app.js",
    "api-server": "cd api-server && npm start",
    "temp-api-server": "node temp-api-server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run api-server\" \"electron . --dev\"",
    "dev-simple": "npm run temp-api-server & electron . --dev",
    "cleanup": "node cleanup-and-exit.js",
    "force-exit": "node cleanup-and-exit.js",
    "build": "electron-packager . ClothingSpaceCapsule --platform=win32 --arch=x64 --out=dist --overwrite --ignore=node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "virtual-fitting",
    "runninghub",
    "comfyui"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^32.3.3",
    "electron-packager": "^17.1.2",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "prisma": "^6.16.2",
    "qrcode": "^1.5.4",
    "uuid": "^13.0.0"
  },
  "build": {}
}
