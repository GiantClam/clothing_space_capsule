# 基础配置
NODE_ENV=development
PORT=4001
API_BASE_URL=http://localhost:4001

# 数据库配置
DATABASE_URL=postgresql://username:password@localhost:5432/clothing_space_capsule

# JWT配置
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=7d

# 腾讯云COS对象存储配置
COS_SECRET_ID=your-cos-secret-id
COS_SECRET_KEY=your-cos-secret-key
COS_BUCKET=your-cos-bucket-name
COS_REGION=ap-guangzhou

# 微信小程序配置
WECHAT_APP_ID=your-wechat-app-id
WECHAT_APP_SECRET=your-wechat-app-secret
WECHAT_TOKEN=your-wechat-token

# RunningHub AI服务配置
RUNNINGHUB_API_KEY=your-runninghub-api-key
RUNNINGHUB_API_URL=https://api.runninghub.ai
RUNNINGHUB_WEBHOOK_SECRET=your-webhook-secret

# 有赞云配置
YOUZAN_CLIENT_ID=your-youzan-client-id
YOUZAN_CLIENT_SECRET=your-youzan-client-secret

# 可选配置（有默认值）
LOG_LEVEL=info
UPLOAD_MAX_SIZE=10485760
SESSION_SECRET=your-session-secret
CORS_ORIGIN=http://localhost:3000